   "source": [
    "def arrayChange(a):\n",
    "    nums_to_update = 0\n",
    "    for i in range(1, len(a)):\n",
    "        if a[i] <= a[i - 1]:\n",
    "            upd = a[i - 1] - a[i] + 1\n",
    "            a[i] += upd\n",
    "            nums_to_update += upd\n",
    "        \n",
    "    return nums_to_update"
   ]
