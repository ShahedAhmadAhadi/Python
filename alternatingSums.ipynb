    "def alternatingSums(a):\n",
    "    lst = [0, 0]\n",
    "    for i, item in enumerate(a):\n",
    "        if i % 2 == 0:\n",
    "            \n",
    "            lst[0] += item\n",
    "        else:\n",
    "            lst[1] += item\n",
    "    \n",
    "    return lst\n",
    "\n",
    "alternatingSums([50, 60, 60, 45, 70])"
   ]
